/* pages/setting/index.wxss */
.se_label{
  display: flex;
  padding: 20rpx 0;
  border-bottom: 1px solid #efefef;
  align-items: center;
}
.sename{
  font-size: 28rpx;
  color: #0b0b0b;
}
.seval{
  flex: 1;
  margin: 0 20rpx;
  text-align: right;
}
.seval input{
  text-align: right;
  font-size: 28rpx;
  color: #b2b2b2;
}
.seval image{
  width: 80rpx; height: 80rpx;
  border-radius: 50%;
}
.seicon image{
  width: 20rpx; height: 38rpx;
}