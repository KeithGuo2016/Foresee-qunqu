/* 申请弹框 */
.apply{
  width: 100%;
  height: auto;
  position: absolute;
  bottom: 0;
  background: #fff;
  padding: 30rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #333;
}
.apply_top{
  display: flex;
  justify-content: space-between;
}
.ap_can{
  color: #666;
}
.ap_ok{
  color: #38bab8;
}
.apply_con{
  margin-top: 30rpx;
}
.apply_con label{
  display: flex;
  margin-bottom: 30rpx;
}
.apply_con .val{
  flex: 1;
  margin-left: 20rpx;
  border-bottom: 1px solid #efefef;
}
.apply_con textarea{
  width: 580rpx;
  margin-left: 20rpx;
  padding: 10rpx;
  line-height: 46rpx;
  height: 200rpx;
  border: 1px solid #efefef;
  box-sizing: border-box;
}
.apply_con .phone{
  flex: 1;
}
.apply_con .code{
  width: 180rpx; height: 40rpx;
  font-size: 30rpx;
  color: #ccc;
  line-height: 40rpx;
  border: 1px solid #ccc;
  border-radius: 40rpx;
}
.apply_con .check{
  color: #38bab8;
  border: 1px solid #38bab8;
}
