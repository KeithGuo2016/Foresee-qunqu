.list_box{
  padding: 0 40rpx;
  overflow: hidden;
}
.returnico {
  display: block;
  z-index: 100;
  width: 50rpx;
  height: 50rpx;
  position: fixed;
  left: 40rpx;
  top: 90rpx;
  background-color: rgba(255, 255, 255, .6);
  border-radius: 50%;
  border: solid 1px #e5e5e5;
}

.returnico:before {
    content: '';
    position: absolute;
    display: block;
    width: 18rpx;
    height: 18rpx;
    left: 50%;
    top: 50%;
    margin-top: -10rpx;
    margin-left: -10rpx;
    border-left: 6rpx solid #000000;
    border-top: 6rpx solid #000000;
    -webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    -o-transform: rotate(-45deg);
    transform: rotate(-45deg);
    background: none;
}
/* 评论弹层 */
.dialog{
  position: fixed;
  z-index: 999;
  width: 100%; height: 100%;
  top: 0; left: 0;
  background: rgba(0, 0, 0, .2);
}
.areabox{
  position: absolute;
  width: 100%;
  background: #fff;
  box-sizing: border-box;
  z-index: 100;
  bottom: 0;
}
.areabtn{
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
  padding: 20rpx 30rpx;
  border-bottom: 1rpx solid #E3E3E3;
}
.cancel{color: #979797;}
.ok{color: #38bab6}
.areabox textarea{
  height: 200rpx;
  padding: 20rpx 30rpx;
  font-size: 28rpx;
  line-break: 42rpx;
  color: #9B9B9B;
}
.fs{
   font-size: 28rpx;
}
.focus{
  width: 130rpx; height: 54rpx;
  display: inline-block;
  line-height: 54rpx;
  background: #fff;
  border-radius: 60rpx;
  font-size: 32rpx;
  color: #000000;
}
.coll_btn{
  width: 690rpx; height: 100rpx;
  margin: 0rpx auto;
  background:#38bab8;
  border-radius: 100rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: #fff;
  line-height: 100rpx;
}

.nodata{
